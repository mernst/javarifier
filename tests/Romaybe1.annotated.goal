import checkers.javari.quals.Mutable;
import checkers.javari.quals.ReadOnly;
import checkers.javari.quals.PolyRead;
public class Romaybe1 {

    public static class Day {
        public int day;
        public Day(@ReadOnly Day this) { }
        public Day(@ReadOnly Day this, int d) { this.day = d; }
        public int getDay(@ReadOnly Day this) /*readonly*/ { return day; }
        public void setDay(@Mutable Day this, int d) /*mutable*/ { this.day = d; }
    }

    public /*romaybe*/ @PolyRead Day id(@ReadOnly Romaybe1 this, /*romaybe*/ @PolyRead Day d) /*readonly*/ {
        return d;
    }

    // forces id not to have readonly returnt type.
    public void foo(@ReadOnly Romaybe1 this, /*mutable*/ @Mutable Day x) /*readonly*/ {
        id(x).setDay(2);
    }
}
