import checkers.javari.quals.Mutable;
import checkers.javari.quals.ReadOnly;
import checkers.javari.quals.ThisMutable;
public class MutableField {

    // That commented @checkers.javari.quals.Mutable is recognized by the extended
    // compiler and supersedes Mutable.fields.
    /*mutable*/ /*@checkers.javari.quals.Mutable*/ Date d;

    // TODO: update this test case once compiler has generic annotations
    // in class file.
    /*this-mutable*/ @ThisMutable Cell</*mutable*/ /*!@checkers.javari.quals.Mutable*/ @ThisMutable Date> f;

    public void foo() /*readonly*/ @ReadOnly {
        d.setDay(3);
    }

    public void bar() /*mutable*/ @Mutable {
        d = new Date();
    }

    public void baz() /*readonly*/ @Mutable {
        /*mutable*/ @Mutable Date x = f.getVal();
        x.setDay(2);
    }

    public void quax() /*mutable*/ @Mutable {
        f.setVal(new Date(3));
    }
}
