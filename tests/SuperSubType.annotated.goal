import checkers.javari.quals.ReadOnly;
import checkers.javari.quals.Mutable;
public class SuperSubType {

  public static class Day {
    public int day;
    public Day(@ReadOnly Day this) { }
    public Day(@ReadOnly Day this, int d) { this.day = d; }
    public int getDay(@ReadOnly Day this) /*readonly*/ { return day; }
    public void setDay(@Mutable Day this, int d) /*mutable*/ { this.day = d; }
  }

  public static class SuperType {

    public void foo(@Mutable SuperType this, @Mutable Day x) {
      x.setDay(3);
    }

  }

  public static class Subtype extends SuperType {

    private Day d;

    public void foo(@Mutable Subtype this, @ReadOnly Day x) {
      d.setDay(3);
    }

  }

}
