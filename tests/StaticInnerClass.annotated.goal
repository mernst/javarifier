import checkers.javari.quals.ReadOnly;
import checkers.javari.quals.Mutable;
/**
 * This class tests static inner classes.
 */
public class StaticInnerClass {

    /*this-mutable*/ Foo f;


    public void bar() /*mutable*/ @Mutable {
        /*readonly*/ @ReadOnly Date d = new Date();
        /*mutable*/ @Mutable Foo x = new Foo(d);
        f = x;
        f.d = new Date();

        /*mutable*/ @Mutable Foo y = new Foo(new Date()); //assigned to this-mut field
        f = y;
        int i = f.d.getDay();

        /*readonly*/ @ReadOnly Foo z = new Foo(new Date());

        int j = f.d.getDay();

    }

    public static class Foo {
        /*readonly*/ @ReadOnly Date d;
        public Foo(/*readonly*/ @ReadOnly Date d) @ReadOnly {
            this.d = d;
        }
    }


}
