import checkers.javari.quals.ReadOnly;
import checkers.javari.quals.Mutable;
/**
 * This class tests static inner classes.
 */
public class StaticInnerClass {

    public static class Day {
        public int day;
        public Day(@ReadOnly Day this) { }
        public Day(@ReadOnly Day this, int d) { this.day = d; }
        public int getDay(@ReadOnly Day this) /*readonly*/ { return day; }
        public void setDay(@Mutable Day this, int d) /*mutable*/ { this.day = d; }
    }

    /*this-mutable*/ Foo f;

    public void bar(@Mutable StaticInnerClass this) /*mutable*/ {
        /*readonly*/ @ReadOnly Day d = new Day();
        /*mutable*/ @Mutable Foo x = new Foo(d);
        f = x;
        f.d = new Day();

        /*mutable*/ @Mutable Foo y = new Foo(new Day()); //assigned to this-mut field
        f = y;
        int i = f.d.getDay();

        /*readonly*/ @ReadOnly Foo z = new Foo(new Day());

        int j = f.d.getDay();

    }

    public static class Foo {
        /*readonly*/ @ReadOnly Day d;
        public Foo(@ReadOnly Foo this, @ReadOnly Day d) {
            this.d = d;
        }
    }


}
