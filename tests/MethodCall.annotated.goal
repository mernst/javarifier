import checkers.javari.quals.Mutable;
import checkers.javari.quals.ReadOnly;
import checkers.javari.quals.PolyRead;
/**
 * This class tests non-parametric, non-static, intra-class method
 * calls.
 */
public class MethodCall {

    private /*this-mutable*/ MethodCall f;

    private /*readonly*/ @ReadOnly MethodCall g;

    private /*romaybe*/ @PolyRead MethodCall getF(@PolyRead MethodCall this) /*romaybe*/ {
        return this.f;
    }

    private /*readonly*/ @ReadOnly MethodCall getG(@ReadOnly MethodCall this) /*readonly*/ {
        return this.g;
    }

    private void setF(@Mutable MethodCall this, /*mutable*/ @Mutable MethodCall mc) /*mutable*/ {
        this.f = mc;
    }

    private void setG(@Mutable MethodCall this, /*readonly*/ @ReadOnly MethodCall mc) /*mutable*/ {
        this.g = mc;
    }


    private void foo(@Mutable MethodCall this) /*mutable*/ {
        /*mutable*/ @Mutable MethodCall x = this.getF();
        x.setF(this);  // forces getFmut() to have mutable return type, and, therefore, mutable reciever.
    }

    private void bar(@ReadOnly MethodCall this) /*readonly*/ {
        /*readonly*/ @ReadOnly MethodCall x = getF();
    }

    private void baz(@Mutable MethodCall this, /*readonly*/ @ReadOnly MethodCall x) /*mutable*/ {
        this.setG(x);
    }

    private void quax(@Mutable MethodCall this, /*mutable*/ @Mutable MethodCall mc) /*mutable*/ {
        this.setF(mc);
    }

    private void deadbeef(@Mutable MethodCall this) /*mutable*/ {
        /*readonly*/ @ReadOnly MethodCall x = this.getG();
        x.bar();
        /*mutable*/ @Mutable MethodCall y = this.getF();
        y.baz(x);
    }

}
